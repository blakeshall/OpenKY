<h2><%= @legislator["full_name"] %></h2>
<img src="<%= @legislator["photo_url"] %>" alt="Photo of <%= @legislator["full_name"] %>" />
<a href="<%= @legislator["url"] %>">Website</a>
<p>Office Address: <%= @legislator["office_address"] %></p>
<p>Office Phone Number: <%= @legislator["office_phone"] %></p>
<p>Party: <%= @legislator["party"] %></p>
<p>District: <%= @legislator["district"] %></p>
<h3>Sponsored Bills</h3>
<% @bills.each do |b| %>
  <p><a href="/bills/<%= URI.encode(b["session"])%>/<%= b["bill_id"] %>" ><%= b["title"] %></a></p>
<% end %>
